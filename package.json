{
  "name": "afinn-96",
  "version": "1.0.0",
  "description": "AFINN 96 (list of English words rated for valence) in JSON",
  "license": "MIT",
  "keywords": [
    "anew",
    "afinn",
    "word",
    "list",
    "sentiment",
    "analysis",
    "opinion",
    "mining",
    "text",
    "microblogs"
  ],
  "files": [
    "index.js",
    "index.json"
  ],
  "repository": "https://github.com/wooorm/afinn-96",
  "bugs": "https://github.com/wooorm/afinn-96/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "dependencies": {},
  "devDependencies": {
    "bail": "^1.0.1",
    "browserify": "^13.0.1",
    "csv-streamify": "^3.0.4",
    "esmangle": "^1.0.1",
    "join-stream": "0.0.0",
    "nyc": "^8.1.0",
    "readable-stream": "^2.1.4",
    "remark-cli": "^1.0.0",
    "remark-comment-config": "^4.0.0",
    "remark-github": "^5.0.0",
    "remark-lint": "^4.0.0",
    "remark-validate-links": "^4.0.0",
    "tape": "^4.0.0",
    "unzip": "^0.1.11",
    "wrap-stream": "^2.0.0",
    "xo": "^0.16.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail",
    "build-generate": "node script/generate.js",
    "build-bundle": "browserify index.js --bare -s afinn96 > afinn-96.js",
    "build-mangle": "esmangle afinn-96.js > afinn-96.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "xo": {
    "space": true,
    "ignores": [
      "afinn-96.js",
      "afinn-96.min.js"
    ]
  },
  "remarkConfig": {
    "output": true,
    "plugins": {
      "comment-config": null,
      "github": null,
      "lint": {
        "list-item-spacing": false
      },
      "validate-links": null
    },
    "settings": {
      "bullet": "*"
    }
  }
}
